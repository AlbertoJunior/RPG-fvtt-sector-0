<div class="S0-message-content">
    <h4 style="text-align: center;">{{localize 'S0.Perseveranca'}}</h4>
    <hr>
    <div class="dice-tooltip S0-container-contract">
        <ol class="dice-rolls">
            {{#each diceValues}}
            <li class="S0-d10 roll d10">{{ this }}</li>
            {{/each}}
        </ol>

        <label style="display: block; margin-block: 6px;">{{localize 'S0.Removidos'}}</label>
        <ol class="dice-rolls">
            {{#each removedDiceValues}}
            <li class="S0-d10-small roll d10">{{ this }}</li>
            {{/each}}
        </ol>
    </div>

    <button class="S0-roll-result {{ resultClassess }}" data-action="toggle-tooltip" data-handle-on-dom="true">
        {{ resultMessage }} ( {{ resultValue }} )
    </button>
</div>