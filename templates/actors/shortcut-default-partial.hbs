<div class="S0-container">
    <h3>{{localize title}}</h3>
    <hr>
    <div class="S0-container">
        <div style="padding-inline: 20px;">
            <h3>{{localize 'S0.Completo'}}</h3>
            <h4>{{actorValues actor actorMethodValue1}}</h4>
        </div>
        <a class="S0-icon-rotate" style="position: absolute; transform: translate(132px, -24px);" data-action="roll"
            data-characteristic="shortcuts" data-sub-characteristic="{{subcharacteristic}}" data-type="{{type1}}">
            <i class="fas fa-dice-d10"></i>
        </a>
    </div>
    <div class="S0-container">
        <div style="padding-inline: 20px;">
            <h3>{{localize 'S0.Dividido'}}</h3>
            <h4>{{actorValues actor actorMethodValue2}}</h4>
        </div>
        <a class="S0-icon-rotate" style="position: absolute; transform: translate(132px, -24px);" data-action="roll"
            data-characteristic="shortcuts" data-sub-characteristic="{{subcharacteristic}}" data-type="{{type2}}">
            <i class="fas fa-dice-d10"></i>
        </a>
    </div>
</div>