{{#unless (operators 'isNull' this.buttons)}}
<div class="S0-flex-center" style="column-gap: 4px; margin-block: 6px">
    {{#each this.buttons}}
    <button class="S0-flex-center {{classes}}" data-action="{{key}}">{{label}}
        {{#unless (operators 'isNull' icon)}}
        <i style="margin: 0px; margin-left: 4px;" class="fas {{icon}}"></i>
        {{/unless}}
    </button>
    {{/each}}
</div>
{{/unless}}